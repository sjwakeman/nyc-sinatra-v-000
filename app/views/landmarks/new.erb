<h1>Add a New Landmark:</h1>

<form action="/landmarks" method="post">
  <label>Name</label>
    <input type="text" name="Name">

  <br><br>

  <label>Figure Name</label>
    <input type="text" name="Figure Name">

  <br><br>

  <% Title.all.each do |title| %>
    <input id="<%= title.name %>" type="checkbox" name="titles[]" value="<%= title.id %>"><%=title.name%></input>
  <% end %>
  <br><br>
    <input type="submit" value="Create">
 </form>
